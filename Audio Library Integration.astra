# Advanced Audio Library: Dynamic Audio Management and Effects
DEFINE FUNCTION load_audio_file(filename):
    IF NOT FILE_EXISTS(filename):
        RETURN CREATE_ERROR("Audio file not found: " + filename)
    audio_data = READ_AUDIO_FILE(filename)
    RETURN CREATE_AUDIO_OBJECT(audio_data)

DEFINE FUNCTION play_audio(audio_object, volume, loop):
    IF audio_object IS NULL:
        RETURN CREATE_ERROR("Invalid audio object.")
    SET_AUDIO_VOLUME(audio_object, volume)
    IF loop:
        audio_object.LOOP = TRUE
    PLAY(audio_object)

DEFINE FUNCTION stop_audio(audio_object):
    IF audio_object IS NULL:
        RETURN CREATE_ERROR("Invalid audio object.")
    STOP(audio_object)

